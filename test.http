POST https://127.0.0.1/back/create_user HTTP/1.1
Content-Type: application/json

{
	"username": "Postname",
	"password": "pass30"
}

// ***************************************************************//
//	     La siguiente petición Obtiene todos los usuarios:        //
// ***************************************************************//

GET https://127.0.0.1/back/get_users  HTTP/1.1


// ***************************************************************//
//	    La siguiente petición Obtiene el usuario por username:    //
// ***************************************************************//

GET https://127.0.0.1/back/get_user_by_username/?username=nombreusuario2 HTTP/1.1


// ***************************************************************//
//	     La siguiente petición elimina usuario por username:      //
// ***************************************************************//

DELETE  https://127.0.0.1/back/delete_user  HTTP/1.1
Content-Type: application/json

{
	"username" : "Postname1"
}

// ***************************************************************//
//	     La siguiente petición elimina usuario por userId:        //
// ***************************************************************//

DELETE  https://127.0.0.1/back/delete_user_by_id  HTTP/1.1
Content-Type: application/json

{
	"userId" : "13"
}

